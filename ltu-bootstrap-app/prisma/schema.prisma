// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Item {
  id          String   @id @default(cuid())
  title       String
  description String?
  html        String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
